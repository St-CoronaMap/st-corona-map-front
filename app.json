{
   "expo": {
      "name": "yotubePlaylist",
      "slug": "youtubePlaylist",
      "scheme": "myapp",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
         "image": "./assets/splash.png",
         "resizeMode": "contain",
         "backgroundColor": "#ffffff"
      },
      "updates": {
         "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
         "supportsTablet": true,
         "bundleIdentifier": "com.callmeshin75.yotubePlaylist"
      },
      "android": {
         "adaptiveIcon": {
            "foregroundImage": "./assets/adaptive-icon.png",
            "backgroundColor": "#FFFFFF"
         },
         "softwareKeyboardLayoutMode": "pan",
         "package": "com.callmeshin75.yotubePlaylist"
      },
      "web": {
         "favicon": "./assets/favicon.png"
      }
   }
}
